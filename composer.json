{
    "name": "jbizzay/pop",
    "description": "Pop app",
    "keywords": ["laravel", "angular"],
    "authors": [
        {
            "name": "Jason Kuchynka",
            "email": "jason.kuchynka@gmail.com",
            "homepage": "http://popwords.net",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "laravelbook/ardent": "dev-master as 2.4.0",
        "zizaco/confide": "3.2.*",
        "zizaco/entrust": "dev-master",
        "andrew13/cabinet": "dev-master",
        "summerstreet/woodling": "dev-master",
        "orchestra/imagine": "dev-master",
        "anahkiasen/underscore-php" : "dev-master",
        "jbizzay/magma": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/libraries"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
